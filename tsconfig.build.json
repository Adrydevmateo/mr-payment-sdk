{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "declaration": true,
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "allowImportingTsExtensions": true
  },
  "include": ["src"]
} 